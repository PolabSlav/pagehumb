                                                    ot(b.start, t)
                                                },
                                                __moveHandles: function(t, e, r) {
                                                    Z(t, e, S, r)
                                                },
                                                options: o,
                                                updateOptions: function(e, t) {
                                                    var r = st(),
                                                        n = ["margin", "limit", "padding", "range", "animate", "snap", "step", "format", "pips", "tooltips"];
