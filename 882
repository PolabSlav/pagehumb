                                        create: function(t, e) {
                                            if (!t || !t.nodeName) throw new Error("rangeSlider (" + lt + "): create requires a single element, got: " + t);
                                            if (t.noUiSlider) throw new Error("rangeSlider (" + lt + "): Slider was already initialized.");
